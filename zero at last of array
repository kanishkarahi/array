#include<iostream>
using namespace std;
int main(){
//void pushzeroes(int arr[],int n)

int a[]={1,2,3,4,0,9,0,8,0};
int n=sizeof(a)/sizeof(a[0]);
int j=0;
for(int i=0;i<n;i++)
{
       if(a[i]!=0){
              swap(a[j],a[i]);
              j++;
       }
}
for(int i=0;i<n;i++)
cout<<a[i]<<" ";

return 0;
}
